local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/EpicDeevv/Private-Scripts/refs/heads/main/th5j6j5j5j", true))()

local window = library:AddWindow("Project V1", {
    main_color = Color3.fromRGB(193, 0, 230),
    min_size = Vector2.new(700, 750),
    can_resize = true,
})

local AutoFarm = window:AddTab("Farm")

AutoFarm:AddLabel("Complements to Farm")

local autoEatEnabled = false

local function eatProteinEgg()
    local player = game.Players.LocalPlayer
    local backpack = player:WaitForChild("Backpack")
    local character = player.Character or player.CharacterAdded:Wait()

    local egg = backpack:FindFirstChild("Protein Egg")
    if egg then
        egg.Parent = character
        pcall(function()
            egg:Activate()
        end)
    end
end

task.spawn(function()
    while true do
        if autoEatEnabled then
            eatProteinEgg()
            task.wait(1800)
        else
            task.wait(1)
        end
    end
end)

AutoFarm:AddSwitch("Auto Eat Egg", function(state)
    autoEatEnabled = state
end)

local repToggle = false

AutoFarm:AddSwitch("Auto Farm (Equip Any tool)", function(state)
    repToggle = state
    while repToggle do
        local args = { "rep" }
        game:GetService("Players").LocalPlayer:WaitForChild("muscleEvent"):FireServer(unpack(args))
        task.wait(0.2)
    end
end)

local folder1 = AutoFarm:AddFolder("Tools")

local weightOn = false
folder1:AddSwitch("Weight", function(bool)
    weightOn = bool
    task.spawn(function()
        while weightOn do
            local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Weight")
            if tool then
                tool.Parent = game.Players.LocalPlayer.Character
            end
            task.wait(0.1)
        end
    end)
end)

